        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryInverse">@color/hover</item>
        <item name="fontFamily">@font/quicksand_font</item>
    </style>

    <style name="Theme.QuychMeal" parent="Base.Theme.QuychMeal" />


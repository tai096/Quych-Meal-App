<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.QuychMeal" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryInverse">@color/hover</item>
        <item name="fontFamily">@font/quicksand_font</item>

    </style>

    <style name="ScreenQuychMeal"><item name="android:background">@color/darkMode</item></style>
    <style name="BorderQuychMeal">
        <item name="android:background">@color/white</item>
    </style>
    <style name="TextQuychMeal"><item name="android:textColor">@color/white</item></style>
    <style name="IconQuychMeal"><item name="android:tint">@color/white</item></style>
    <style name="CustomIndicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">@color/transparent</item>
    </style>
    <style name="CardQuychMeal">
        <item name="android:backgroundTint">#424242</item>
    </style>
    <style name="Card2QuychMeal">
        <item name="android:background">#424242</item>
    </style>
</resources>